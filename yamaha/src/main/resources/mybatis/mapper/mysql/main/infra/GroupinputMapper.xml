<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.a3a3.fapp.groupinput.GroupinputDao">

	<resultMap id="resultMapObj" type="com.a3a3.fapp.groupinput.GroupinputDto"></resultMap>
	  
	<select id="selectList" resultMap="resultMapObj">
		SELECT
			seq,
		    groupName,
		    representGroup
		FROM
			groupinput
		WHERE 1=1
		ORDER BY seq DESC
	
	</select>
	
</mapper>